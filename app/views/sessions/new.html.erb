<h1>ログイン</h1>
<%= form_with url: login_path, local: true do |form| %>
  <div>
    <%= form.label :ユーザ名 %>
    <%= form.text_field :username %>
  </div>

  <div>
    <%= form.label :パスワード %>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.submit "ログインする" %>
  </div>
<% end %>
